{
    "version": 3,
    "name": "API Gateway",
    "port": 8080,
    "timeout": "3000ms",
    "extra_config": {
    "router": {
      "disable_gzip": false
    }
  },
    "endpoints": [
      {
        "endpoint": "/users/{user}",
        "method": "GET",
        "output_encoding": "json",
   
        "backend": [
          {
            "host": [
              "https://jsonplaceholder.typicode.com"
            ],
            "encoding": "json",
            "url_pattern": "/users/{user}",
            "group": "user",
            "deny": [
                "id"
              ]
          }
        ]
      },
      {
        "endpoint": "/posts/{user}",
        "method": "GET",
        "output_encoding": "json",
        "backend": [
          {
            "host": [
              "https://jsonplaceholder.typicode.com"
            ],
            "encoding": "json",
            "url_pattern": "/posts/{user}",
            "deny": [
                "id",
                "userId"
              ],
              "group": "last_post"
          }
        ]
      }
    ]
  }
  